                this.settings.autoplay === !0 ? (this.on("gotoSlide." + i, t.proxy(this._autoplayOnGotoSlide, this)), this.on("mouseenter." + i, t.proxy(this._autoplayOnMouseEnter, this)), this.on("mouseleave." + i, t.proxy(this._autoplayOnMouseLeave, this)), this.startAutoplay()) : (this.off("gotoSlide." + i), this.off("mouseenter." + i), this.off("mouseleave." + i), this.stopAutoplay())
