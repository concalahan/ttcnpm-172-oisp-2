    function(n) {
        var o = {
                topSpacing: 5,
                bottomSpacing: 580,
                className: "is-sticky",
                wrapperClassName: "sticky-wrapper"
            },
            i = n(window),
            s = n(document),
            r = [],
            f = i.height(),
