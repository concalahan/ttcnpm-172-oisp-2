                clearTimeout(this.autoplayTimer);
                this.off("update." + i);
                this.off("gotoSlide." + i);
                this.off("mouseenter." + i);
                this.off("mouseleave." + i)
            },
            autoplayDefaults: {
                autoplay: !0,
                autoplayDelay: 5e3,
                autoplayDirection: "normal",
                autoplayOnHover: "pause"
            }
        };
